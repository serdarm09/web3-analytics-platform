    {
      "path": "/api/cron",
      "schedule": "0 */30 * * * *"
    },
    {
      "path": "/api/cron/sync-trending",
      "schedule": "0 */15 * * * *"
    }
  ],
  "functions": {
    "app/api/sync/crypto-data/route.ts": {
      "maxDuration": 60
